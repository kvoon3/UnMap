{
  "name": "@kvoon/unmap",
  "type": "module",
  "version": "0.0.2-beta.0",
  "packageManager": "pnpm@8.14.1",
  "description": "_description_",
  "author": "Kevin Kwong <a1176079417@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/kvoon3/unmap#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kvoon3/unmap.git"
  },
  "bugs": "https://github.com/kvoon3/unmap/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "play": "nr -C playground dev",
    "prepare": "simple-git-hooks",
    "prepublishOnly": "nr build",
    "release": "bumpp && pnpm -r publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "consola": "^3.2.3",
    "long": "^5.2.3",
    "nanoid": "^5.0.4"
  },
  "devDependencies": {
    "@amap/amap-jsapi-types": "^0.0.13",
    "@antfu/eslint-config": "^2.6.3",
    "@antfu/utils": "^0.7.7",
    "@playwright/test": "^1.41.0",
    "@types/geojson": "^7946.0.13",
    "@types/mapbox-gl": "^2.7.19",
    "@types/node": "^20.11.5",
    "bumpp": "^9.3.0",
    "eslint": "^8.56.0",
    "esno": "^4.0.0",
    "jiti": "^1.21.0",
    "lint-staged": "^15.2.0",
    "pnpm": "^8.14.1",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.9.0",
    "taze": "^0.13.1",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vite": "^5.0.11",
    "vitest": "^1.2.1",
    "vue-tsc": "^1.8.27"
  },
  "resolutions": {
    "jiti": "./jiti-1.19.1.tgz",
    "unbuild": "./unbuild-2.0.0-rc.0.tgz"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
