{
  "name": "@kvoon/unmap",
  "type": "module",
  "version": "0.0.2-beta.0",
  "packageManager": "pnpm@9.1.2",
  "description": "_description_",
  "author": "Kevin Kwong <a1176079417@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/kvoon3/unmap#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kvoon3/unmap.git"
  },
  "bugs": "https://github.com/kvoon3/unmap/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "play": "nr -C playground dev",
    "prepare": "simple-git-hooks",
    "prepublishOnly": "nr build",
    "release": "bumpp && pnpm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "consola": "^3.2.3",
    "long": "^5.2.3",
    "nanoid": "^5.0.7"
  },
  "devDependencies": {
    "@amap/amap-jsapi-types": "^0.0.15",
    "@antfu/eslint-config": "^2.19.0",
    "@antfu/utils": "^0.7.8",
    "@playwright/test": "^1.44.1",
    "@types/geojson": "^7946.0.14",
    "@types/mapbox-gl": "^3.1.0",
    "@types/node": "^20.12.12",
    "bumpp": "^9.4.1",
    "eslint": "^9.3.0",
    "esno": "^4.7.0",
    "jiti": "^1.21.0",
    "lint-staged": "^15.2.5",
    "pnpm": "^9.1.2",
    "rimraf": "^5.0.7",
    "simple-git-hooks": "^2.11.1",
    "taze": "^0.13.8",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "vite": "^5.2.11",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.19"
  },
  "resolutions": {
    "jiti": "./jiti-1.19.1.tgz",
    "unbuild": "./unbuild-2.0.0-rc.0.tgz"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
