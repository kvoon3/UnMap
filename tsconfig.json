{
  "compilerOptions": {
    "target": "es2018",
    "lib": ["esnext", "DOM"],
    "baseUrl": ".",
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "types": [
      "@amap/amap-jsapi-types",
      "@types/geojson",
      "@types/mapbox-gl",
      "@types/node"
    ],
    "strict": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true
  }
}
